<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <!-- Allow cleartext HTTP traffic for templerun.click if needed -->
    <domain-config cleartextTrafficPermitted="true">
        <domain includeSubdomains="true">templerun.click</domain>
    </domain-config>
    <!-- Default: secure (HTTPS only) -->
    <base-config cleartextTrafficPermitted="false">
        <trust-anchors>
            <certificates src="system" />
        </trust-anchors>
    </base-config>
</network-security-config>
